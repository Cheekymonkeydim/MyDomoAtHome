    install)
        npm install -g forever
        npm install sync-request
        npm install npm-install-missing
        ;;

    upgrade|abort-upgrade)
        cd /usr/share/mydomoathome/app/ && npm-install-missing
        ;;
        
    configure)
        ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 0
        ;;
esac

#DEBHELPER#

exit 0
